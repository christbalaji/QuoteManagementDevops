services:
  quotemanagement:
    build:
      context: C:\Users\dotnetcloudmc65\source\repos\ClientManagementDevops\QuoteManagement
      dockerfile: Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: QuoteManagement
    container_name: QuoteManagement
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: quotemanagement
    labels:
      com.microsoft.visual-studio.project-name: QuoteManagement
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/QuoteManagement.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - 80/tcp
    - 443/tcp
    tty: true
    volumes:
    - C:\Users\dotnetcloudmc65\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\dotnetcloudmc65\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\dotnetcloudmc65\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
version: '3.4'